{
  "name": "materialish",
  "version": "0.0.5",
  "description": "React components that loosely follow Material Design",
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "test": "npm run lint",
    "lint": "eslint components stories",
    "prepublish": "in-publish && npm run build || not-in-publish",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "clean": "rimraf dist es tmp lib",
    "prettier": "prettier --config ./.prettierrc --write **/*.{js,css} components/**/*.js",
    "storybook:clean": "rimraf docs",
    "storybook:build": "build-storybook -o docs",
    "storybook:publish": "npm run storybook:clean && npm run storybook:build && cd docs && git init && git commit --allow-empty -m 'update storybook' && git checkout -b gh-pages && touch .nojekyll && git add . && git commit -am 'update book' && git push git@github.com:jamesplease/materialish gh-pages --force",
    "build": "npm run clean && npm run build:umd && npm run build:umd:min && npm run build:es && npm run build:commonjs",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build:umd": "cross-env NODE_ENV=development BABEL_ENV=build rollup -c -i src/index.js -o dist/materialish.js",
    "build:umd:min": "cross-env NODE_ENV=production BABEL_ENV=buildProd rollup -c -i src/index.js -o dist/materialish.min.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jamesplease/materialish.git"
  },
  "keywords": [
    "react",
    "components",
    "material",
    "design",
    "ux",
    "ui"
  ],
  "author": "Javier Porrero <porrero.javier@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jamesplease/materialish/issues"
  },
  "files": [
    "dist",
    "lib",
    "es"
  ],
  "homepage": "https://jamesplease.github.io/materialish",
  "dependencies": {
    "classnames": "^2.2.5",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.15",
    "@storybook/addon-knobs": "^3.3.15",
    "@storybook/addon-links": "^3.3.15",
    "@storybook/addon-notes": "^3.3.15",
    "@storybook/addon-options": "^3.3.15",
    "@storybook/addons": "^3.3.15",
    "@storybook/react": "^3.3.14",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.1.4",
    "eslint": "^4.19.0",
    "eslint-plugin-react": "^7.7.0",
    "in-publish": "^2.0.0",
    "prettier": "^1.11.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.57.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^3.0.0",
    "storybook-addon-jsx": "^5.3.0"
  }
}
